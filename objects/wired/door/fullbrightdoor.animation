{
  "globalTagDefaults" : {
    "doorDirection" : "Right"
  },

  "animatedParts" : {
    "stateTypes" : {
      "doorState" : {
        "default" : "closed",
        "states" : {
          "open" : {
            "frames" : 2,
            "cycle" : 0.15
          },
          "closing" : {
            "frames" : 2,
            "cycle" : 0.15,
            "mode" : "transition",
            "transition" : "closed"
          },
          "closed" : { },
          "locking" : {
            "frames" : 2,
            "cycle" : 0.15,
            "mode" : "transition",
            "transition" : "locked"
          },
          "locked" : { }
        }
      }
    },

    "parts" : {
      "bg" : {
        "properties" : {
          "centered" : false,
          "zLevel" : 1
        },

        "partStates" : {
          "doorState" : {
            "open" : {
              "properties" : {
                "image" : "<partImage>:open<doorDirection>.<frame>"
              }
            },

            "closing" : {
              "properties" : {
                "image" : "<partImage>:close<doorDirection>.<frame>"
              }
            },

            "closed" : {
              "properties" : {
                "image" : "<partImage>:close<doorDirection>.2"
              }
            },

            "locking" : {
              "properties" : {
                "image" : "<partImage>:close<doorDirection>.<frame>"
              }
            },

            "locked" : {
              "properties" : {
                "image" : "<partImage>:close<doorDirection>.2"
              }
            }
          }
        }
      },

      "fg" : {
        "properties" : {
          "fullbright" : true,
          "centered" : false,
          "zLevel" : 2
        },

        "partStates" : {
          "doorState" : {
            "open" : {
              "properties" : {
                "image" : "<partImage>:open<doorDirection>.<frame>"
              }
            },

            "closing" : {
              "properties" : {
                "image" : "<partImage>:close<doorDirection>.<frame>"
              }
            },

            "closed" : {
              "properties" : {
                "image" : "<partImage>:close<doorDirection>.2"
              }
            },

            "locking" : {
              "properties" : {
                "image" : "<partImage>:close<doorDirection>.<frame>"
              }
            },

            "locked" : {
              "properties" : {
                "image" : "<partImage>:close<doorDirection>.2"
              }
            }
          }
        }
      }
    }
  },

  "lights" : {
    "fg" : {
      "active" : true,
      "color" : [130, 217, 232],
      "position" : [0.0, 5.0]
    }
  },

  "sounds" : {
    "open" : [],
    "close" : [],
    "locked" : [ "/sfx/interface/clickon_error.ogg" ]
  }
}
